        <source>Quick-search Engines</source>
        <target>快速搜尋引擎</target>
        <note>Title for quick-search engines settings section.</note>
      </trans-unit>
      <trans-unit id="Reader">
        <source>Reader</source>
        <target>閱讀器</target>
        <note>Browser function that presents simplified version of the page with bigger text.</note>
      </trans-unit>
      <trans-unit id="Reading list">
        <source>Reading list</source>
        <target>閱讀清單</target>
        <note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="Recent Bookmarks">
        <source>Recent Bookmarks</source>
        <target>最近使用的書籤</target>
        <note>Header for bookmarks list</note>
      </trans-unit>
      <trans-unit id="Search">
        <source>Search</source>
        <target>搜尋</target>
      </trans-unit>
      <trans-unit id="Search Settings">
        <source>Search Settings</source>
        <target>搜尋設定</target>
        <note>Title for search settings section.</note>
      </trans-unit>
      <trans-unit id="Search or enter address">
        <source>Search or enter address</source>
        <target>搜尋或輸入網址</target>
        <note>The text shown in the URL bar on about:home</note>
      </trans-unit>
      <trans-unit id="Secure connection">
        <source>Secure connection</source>
        <target>安全連線</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Settings">
        <source>Settings</source>
        <target>設定</target>
      </trans-unit>
      <trans-unit id="Show Tabs">
        <source>Show Tabs</source>
        <target>顯示分頁</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Show search suggestions">
        <source>Show search suggestions</source>
        <target>顯示搜尋建議</target>
        <note>Label for show search suggestions setting.</note>
      </trans-unit>
      <trans-unit id="Sign in">
        <source>Sign in</source>
        <target>登入</target>
      </trans-unit>
      <trans-unit id="Tabs Tray">
        <source>Tabs Tray</source>
        <target>分頁列</target>
        <note>Accessibility label for the Tabs Tray view.</note>
      </trans-unit>
      <trans-unit id="Today">
        <source>Today</source>
        <target>今天</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Top sites">
        <source>Top sites</source>
        <target>最常開啟的網站</target>
        <note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="URL">
        <source>URL</source>
        <target>網址</target>
        <note>Accessibility label</note>
      </trans-unit>
      <trans-unit id="Upgrade Firefox to connect.">
        <source>Upgrade Firefox to connect.</source>
        <target>升級 Firefox 以連線。</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Verify your email address.">
        <source>Verify your email address.</source>
        <target>驗證您的電子郵件地址。</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Web content">
        <source>Web content</source>
        <target>網頁內容</target>
        <note>Accessibility label for the main web content view</note>
      </trans-unit>
      <trans-unit id="Yesterday">
        <source>Yesterday</source>
        <target>昨天</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
    </body>

        <source>Last week</source>
        <target>上週</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="OK">
        <source>OK</source>
        <target>確定</target>
        <note>OK button</note>
      </trans-unit>

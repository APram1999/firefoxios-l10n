  <file original="Client/Localizable.strings" source-language="en" datatype="plaintext">
    <header>
      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="6.1.1" build-num="6A2008a"/>
    </header>
    <body>
      <trans-unit id="%@ search" approved="yes">
        <source>%@ search</source> <target state="translated">%@ 検索</target><note>Label for search engine buttons. The argument corresponds to the name of the search engine.</note>
      </trans-unit>
      <trans-unit id="Add to Bookmarks" approved="yes">
        <source>Add to Bookmarks</source> <target state="translated">ブックマークに追加</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Add to Reading List" approved="yes">
        <source>Add to Reading List</source> <target state="translated">リーディングリストに追加</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Address and Search" approved="yes">
        <source>Address and Search</source> <target state="translated">アドレスと検索</target><note>Accessibility label for address and search field, both words (Address, Search) are therefore nouns.</note>
      </trans-unit>
      <trans-unit id="Back" approved="yes">
        <source>Back</source> <target state="translated">戻る</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Bookmarks" approved="yes">
        <source>Bookmarks</source> <target state="translated">ブックマーク</target><note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="Cancel" approved="yes">
        <source>Cancel</source> <target state="translated">キャンセル</target></trans-unit>
      <trans-unit id="Could not connect to Firefox Accounts." approved="yes">
        <source>Could not connect to Firefox Accounts.</source> <target state="translated">Firefox アカウントに接続できませんでした。</target><note>Settings</note>
      </trans-unit>
      <trans-unit id="Default Search Engine" approved="yes">
        <source>Default Search Engine</source> <target state="translated">デフォルト検索エンジン</target></trans-unit>
      <trans-unit id="Disconnect" approved="yes">
        <source>Disconnect</source> <target state="translated">切断</target><note>Settings</note>
      </trans-unit>
      <trans-unit id="Disconnect Firefox Account?" approved="yes">
        <source>Disconnect Firefox Account?</source> <target state="translated">Firefox アカウントを切断しますか？</target><note>Settings</note>
      </trans-unit>
      <trans-unit id="Done" approved="yes">
        <source>Done</source> <target state="translated">完了</target><note>Settings</note>
      </trans-unit>
      <trans-unit id="Double tap and hold to open history" approved="yes">
        <source>Double tap and hold to open history</source> <target state="translated">ダブルタップして長押しで履歴を開きます</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Enter your password to connect." approved="yes">
        <source>Enter your password to connect.</source> <target state="translated">接続のためパスワードを入力してください。</target><note>Settings</note>
      </trans-unit>
      <trans-unit id="Firefox will stop syncing with your account, but won’t delete any of your browsing data on this device." approved="yes">
        <source>Firefox will stop syncing with your account, but won’t delete any of your browsing data on this device.</source> <target state="translated">Firefox はあなたのアカウントとの同期を中止しますが、この端末上のブラウズ履歴は削除されません。</target><note>Settings</note>
      </trans-unit>
      <trans-unit id="Forward" approved="yes">
        <source>Forward</source> <target state="translated">進む</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="History" approved="yes">
        <source>History</source> <target state="translated">履歴</target><note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="Last month" approved="yes">
        <source>Last month</source> <target state="translated">先月</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Last week" approved="yes">
        <source>Last week</source> <target state="translated">先週</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="OK" approved="yes">
        <source>OK</source> <target state="translated">OK</target><note>OK button</note>
      </trans-unit>
      <trans-unit id="Quick-search Engines" approved="yes">
        <source>Quick-search Engines</source> <target state="translated">クイック検索エンジン</target><note>Title for quick-search engines settings section.</note>
      </trans-unit>
      <trans-unit id="Reader" approved="yes">
        <source>Reader</source> <target state="translated">リーダー</target><note>Browser function that presents simplified version of the page with bigger text.</note>
      </trans-unit>
      <trans-unit id="Reading list" approved="yes">
        <source>Reading list</source> <target state="translated">リーディングリスト</target><note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="Recent Bookmarks" approved="yes">
        <source>Recent Bookmarks</source> <target state="translated">最近のブックマーク</target><note>Header for bookmarks list</note>
      </trans-unit>
      <trans-unit id="Search" approved="yes">
        <source>Search</source> <target state="translated">検索</target></trans-unit>
      <trans-unit id="Search Settings" approved="yes">
        <source>Search Settings</source> <target state="translated">検索設定</target><note>Title for search settings section.</note>
      </trans-unit>
      <trans-unit id="Search or enter address" approved="yes">
        <source>Search or enter address</source> <target state="translated">検索語またはアドレスを入力</target><note>The text shown in the URL bar on about:home</note>
      </trans-unit>
      <trans-unit id="Secure connection" approved="yes">
        <source>Secure connection</source> <target state="translated">安全な接続</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Send To Device" approved="yes">
        <source>Send To Device</source> <target state="translated">端末に送信</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Send to " approved="yes">
        <source>Send to</source> <target state="translated">送信先</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Settings" approved="yes">
        <source>Settings</source> <target state="translated">設定</target></trans-unit>
      <trans-unit id="Show Tabs" approved="yes">
        <source>Show Tabs</source> <target state="translated">タブを表示</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Show search suggestions" approved="yes">
        <source>Show search suggestions</source> <target state="translated">検索候補を表示</target><note>Label for show search suggestions setting.</note>
      </trans-unit>
      <trans-unit id="Sign in" approved="yes">
        <source>Sign in</source> <target state="translated">サインイン</target></trans-unit>
      <trans-unit id="Tabs Tray">
        <source>Tabs Tray</source> <target state="needs-review-translation">タブトレイ</target><note>Accessibility label for the Tabs Tray view.</note>
      </trans-unit>
      <trans-unit id="Today" approved="yes">
        <source>Today</source> <target state="translated">今日</target><note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Top sites">
        <source>Top sites</source> <target state="needs-review-translation">トップサイト</target><note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="URL" approved="yes">
        <source>URL</source> <target state="translated">URL</target><note>Accessibility label</note>
      </trans-unit>
      <trans-unit id="Upgrade Firefox to connect." approved="yes">
        <source>Upgrade Firefox to connect.</source> <target state="translated">接続するには Firefox をアップグレードしてください。</target><note>Settings</note>
      </trans-unit>
      <trans-unit id="Verify your email address." approved="yes">
        <source>Verify your email address.</source> <target state="translated">メールアドレスを確認してください。</target><note>Settings</note>
      </trans-unit>
      <trans-unit id="Web content" approved="yes">
        <source>Web content</source> <target state="translated">Web コンテンツ</target><note>Accessibility label for the main web content view</note>
      </trans-unit>
      <trans-unit id="Yesterday" approved="yes">
        <source>Yesterday</source> <target state="translated">昨日</target><note>No comment provided by engineer.</note>
      </trans-unit>
    </body>
  </file>

  <file original="Client/Localizable.strings" source-language="en" datatype="plaintext" target-language="sk">
    <header>
      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="6.2" build-num="6C131e"/>
    </header>
    <body>
      <trans-unit id="%@ search">
        <source>%@ search</source>
        <target>Vyhľadávanie %@</target>
        <note>Label for search engine buttons. The argument corresponds to the name of the search engine.</note>
      </trans-unit>
      <trans-unit id="Address and Search">
        <source>Address and Search</source>
        <target>Adresa a vyhľadávanie</target>
        <note>Accessibility label for address and search field, both words (Address, Search) are therefore nouns.</note>
      </trans-unit>
      <trans-unit id="Back">
        <source>Back</source>
        <target>Naspäť</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Bookmarks">
        <source>Bookmarks</source>
        <target>Záložky</target>
        <note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="Cancel">
        <source>Cancel</source>
        <target>Zrušiť</target>
      </trans-unit>
      <trans-unit id="Could not connect to Firefox Accounts.">
        <source>Could not connect to Firefox Accounts.</source>
        <target>Nebolo možné pripojiť sa k účtu Firefox Account.</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Default Search Engine">
        <source>Default Search Engine</source>
        <target>Predvolený vyhľadávací modul</target>
      </trans-unit>
      <trans-unit id="Disconnect">
        <source>Disconnect</source>
        <target>Odpojiť</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Disconnect Firefox Account?">
        <source>Disconnect Firefox Account?</source>
        <target>Odpojiť sa z účtu Firefox Account?</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Done">
        <source>Done</source>
        <target>Hotovo</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Double tap and hold to open history">
        <source>Double tap and hold to open history</source>
        <target>Históriu otvoríte dvojitým ťuknutím a podržaním</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Enter your password to connect.">
        <source>Enter your password to connect.</source>
        <target>Na pripojenie zadajte svoje heslo</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Firefox will stop syncing with your account, but won’t delete any of your browsing data on this device.">
        <source>Firefox will stop syncing with your account, but won’t delete any of your browsing data on this device.</source>
        <target>Firefox sa už viac nebude synchronizovať s vašim účtom, ale neodstráni z toho zariadenia žiadne z údajov prehliadania.</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Forward">
        <source>Forward</source>
        <target>Dopredu</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="History">
        <source>History</source>
        <target>História</target>
        <note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="Last month">
        <source>Last month</source>
        <target>Posledný mesiac</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Last week">
        <source>Last week</source>
        <target>Posledný týždeň</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="OK">
        <source>OK</source>
        <target>OK</target>
        <note>OK button</note>
      </trans-unit>
      <trans-unit id="Quick-search Engines">
        <source>Quick-search Engines</source>
        <target>Moduly pre rýchle vyhľadávanie</target>
        <note>Title for quick-search engines settings section.</note>
      </trans-unit>
      <trans-unit id="Reader">
        <source>Reader</source>
        <target>Čítačka</target>
        <note>Browser function that presents simplified version of the page with bigger text.</note>
      </trans-unit>
      <trans-unit id="Reading list">
        <source>Reading list</source>
        <target>Zoznam na prečítanie</target>
        <note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="Recent Bookmarks">
        <source>Recent Bookmarks</source>
        <target>Najnovšie záložky</target>
        <note>Header for bookmarks list</note>
      </trans-unit>
      <trans-unit id="Search">
        <source>Search</source>
        <target>Vyhľadávanie</target>
      </trans-unit>
      <trans-unit id="Search Settings">
        <source>Search Settings</source>
        <target>Nastavenie vyhľadávania</target>
        <note>Title for search settings section.</note>
      </trans-unit>
      <trans-unit id="Search or enter address">
        <source>Search or enter address</source>
        <target>Zadajte vyhľadávanie alebo adresu</target>
        <note>The text shown in the URL bar on about:home</note>
      </trans-unit>
      <trans-unit id="Secure connection">
        <source>Secure connection</source>
        <target>Bezpečné pripojenie</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Settings">
        <source>Settings</source>
        <target>Nastavenia</target>
      </trans-unit>
      <trans-unit id="Show Tabs">
        <source>Show Tabs</source>
        <target>Zobraziť karty</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Show search suggestions">
        <source>Show search suggestions</source>
        <target>Zobrazovať návrhy vyhľadávania</target>
        <note>Label for show search suggestions setting.</note>
      </trans-unit>
      <trans-unit id="Sign in">
        <source>Sign in</source>
        <target>Prihlásiť sa</target>
      </trans-unit>
      <trans-unit id="Tabs Tray">
        <source>Tabs Tray</source>
        <target>Tray kariet</target>
        <note>Accessibility label for the Tabs Tray view.</note>
      </trans-unit>
      <trans-unit id="Today">
        <source>Today</source>
        <target>Dnes</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
      <trans-unit id="Top sites">
        <source>Top sites</source>
        <target>Top stránky</target>
        <note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="URL">
        <source>URL</source>
        <target>Adresa URL</target>
        <note>Accessibility label</note>
      </trans-unit>
      <trans-unit id="Upgrade Firefox to connect.">
        <source>Upgrade Firefox to connect.</source>
        <target>Aby ste sa mohli pripojiť, aktualizujte Firefox.</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Verify your email address.">
        <source>Verify your email address.</source>
        <target>Overte svoju e-mailovú adresu.</target>
        <note>Settings</note>
      </trans-unit>
      <trans-unit id="Web content">
        <source>Web content</source>
        <target>Webový obsah</target>
        <note>Accessibility label for the main web content view</note>
      </trans-unit>
      <trans-unit id="Yesterday">
        <source>Yesterday</source>
        <target>Včera</target>
        <note>No comment provided by engineer.</note>
      </trans-unit>
    </body>

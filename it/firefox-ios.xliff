      </trans-unit>
      <trans-unit id="Sign in">
        <source>Sign in</source>
        <target>Accedi</target>
      </trans-unit>
      <trans-unit id="Tabs Tray">
        <source>Tabs Tray</source>
        <target>Area delle schede</target>
